<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.palak.hospitalmanagement">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="hospitalmanagement">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Register"
            android:label="@string/title_activity_register" />
        <activity
            android:name=".mainpatient"
            android:label="@string/title_activity_mainpatient"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".maindoctor"
            android:label="@string/title_activity_maindoctor"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>